{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@root/*": ["./*"],
      "@assets/*": ["assets/*"],
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@actions/*": ["src/actions/*"],
      "@config/*": ["src/config/*"],
    },
    "strict": true
  },
  "skipLibCheck": true
}